# Configurações do Banco de Dados PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=net_imobiliaria
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui

# Configurações do Gmail SMTP para Desenvolvimento
# IMPORTANTE: Use uma senha de app específica, não sua senha normal do Gmail
# Para gerar: https://myaccount.google.com/apppasswords
GMAIL_USER=seu_email@gmail.com
GMAIL_APP_PASSWORD=sua_senha_de_app_do_gmail

# Configurações de Email
EMAIL_FROM_NAME="Net Imobiliária"
EMAIL_FROM_ADDRESS=seu_email@gmail.com

# Configurações de 2FA
JWT_SECRET=sua_chave_secreta_jwt_muito_forte_aqui_para_desenvolvimento
JWT_EXPIRES_IN=1h

# Configurações de Rate Limiting
RATE_LIMIT_MAX_ATTEMPTS=5
RATE_LIMIT_WINDOW_MS=900000

# Configurações de Sessão
SESSION_SECRET=sua_session_secret_muito_forte_aqui
SESSION_MAX_AGE=86400000

# Configurações de Desenvolvimento
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000